<div class="col-md-8"
<h1>Sign Up Confirmation</h1>
<h3>Please confirm that Name and Email are correct.</h3>

<h4>Name: <%= @user.name %></h4>
<h4>Email: <%= @user.email %></h4>

<div class="row">
<div class="col-xs-1">
<%= button_to "Yes", {controller: "users", action: "create", params: params}, class: 'btn btn-success' %>
</div>
<div class="col-xs-1">
<%= button_to "No", {controller: "users", action: "new"}, class: 'btn btn-failure' %>
</div>
</div>
</div>
